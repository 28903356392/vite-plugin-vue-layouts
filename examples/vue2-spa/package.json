{
  "name": "vite-vue2-typescript",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint './src/**/*.{js,ts,tsx,vue,md}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx,vue,md}' --fix",
    "format": "pretty-quick --staged --ignore-path .gitignore"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.9",
    "@vueuse/core": "^4.11.2",
    "core-js": "^3.21.1",
    "element-ui": "^2.15.6",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vue2-helpers": "^1.1.7",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/runtime-dom": "^3.2.31",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "unplugin-vue-components": "^0.14.13",
    "unplugin-vue2-script-setup": "^0.4.2",
    "vite": "^2.8.6",
    "vite-plugin-inspect": "^0.2.2",
    "vite-plugin-optimize-persist": "^0.0.5",
    "vite-plugin-package-config": "^0.0.3",
    "vite-plugin-pages": "^0.17.5",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-vue2": "^1.9.3",
    "vite-plugin-windicss": "^1.8.1",
    "vue-demi": "^0.7.5",
    "vue-template-compiler": "^2.6.14",
    "windicss": "^3.5.0"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@vue/composition-api",
        "@vue/runtime-dom",
        "element-ui",
        "element-ui/lib/button",
        "element-ui/lib/divider",
        "element-ui/lib/menu",
        "element-ui/lib/menu-item",
        "element-ui/lib/tag",
        "vue",
        "vue-router",
        "vue2-helpers/vue-router",
        "vuex"
      ]
    }
  }
}
